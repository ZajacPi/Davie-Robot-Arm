<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="urdf4">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import urdf4 urdf file -->
    <xacro:include filename="$(find urdf4_description)/urdf/urdf4.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="urdf4.ros2_control.xacro" />


    <xacro:urdf4_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
