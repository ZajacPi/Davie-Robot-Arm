<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="David_assembly_body" args="Revolute 2_position_controller Revolute 4_position_controller Revolute 5_position_controller Revolute 6_position_controller Revolute 8_position_controller joint_state_controller "/>
